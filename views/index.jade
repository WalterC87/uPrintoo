extends layout

block content
	include menu
	//--nav#navigation
		div.container
			div.row-fluid
				div.span12.center
					a.brand.pull-left(href='./')
						img(src='/img/uPrintoologo.png', alt='Printoo', width='143')
					div.mobile-menu(data-toggle='collapse',data-target='.nav-collapse')
						span.icon-bar
						span.icon-bar
						span.icon-bar
					ul#main-menu.nav-collapse.collapse.pull-right
						li
							p COMPARTE
						li
							a.facebook-share(href='https://www.facebook.com/pages/Printoo/1475470072669968', target="_blank")

						li
							a.twitter-share(href='https://twitter.com/printoome', target='_blank')

		//--div.overlay
	section#portada.slide
		section.img-portada
			//--img(src='/img/portadas/portada.jpg', width='100%', height='100%')
		section.centrada
			div
				div.row-fluid.center
					div.info-principal.span12
						h1.texto-principal Mensualmente manda a imprimir <br> y recibe en tu casa tus fotos favoritas.
						h3.sub-principal Sin pagar nada. 
						<br><br><br><br><br><br><br><br>
						div.span5.center.offset5.godown
							a.arrow-down(href='#div-2')

	section#div-2.slide
		section.info-portada
			div.container
				div.row-fluid.center
					<br><br>
					h3.texto-portada 7 de cada 10 personas han <br> fotografiado momentos increíbles.
					<br><br><br><br><br><br><br><br>
					div.span5.center.offset5.godown
						a.arrow-down(href='#div-3')
		section.img-portada
			//--img(src='/img/portadas/skater.jpg', width='100%', height='100%')

	section#div-3.slide
		section.info-portada
			div.container
				div.row-fluid.center
					<br><br>
					h3.texto-portada 8 de cada 10 viajeros tomaron fotos <br> que valen la pena imprimir.
					<br><br><br><br><br><br><br><br><br>
					div.span2.center.offset5.godown
						a.arrow-down(href='#div-4')
		section.img-portada
			//--img(src='/img/portadas/ban.jpg', width='100%', height='100%')

	section#div-4.slide
		section.info-portada
			div.container
				div.row-fluid.center
					<br><br>
					h3.texto-portada 6 de cada 10 personas han borrado <br> digitalmente fotos irrebatibles.
					<br><br><br><br><br><br><br><br><br>
					div.span5.center.offset5.godown
						a.arrow-down(href='#div-5')
		section.img-portada
			//--img(src='/img/portadas/celular.jpg', width='100%', height='100%')
	
	section#div-5.slide
		section.info-portada
			div.container
				div.row-fluid.center
					<br><br>
					h3.texto-portada 4 de 5 cada  enamorados regalarían más fotos <br> si fuera más fácil imprimirlas.
					<br><br><br><br><br><br><br><br><br>
					div.span5.center.offset5.godown
						a.arrow-down(href='#div-6')
		section.img-portada
			//--img(src='/img/portadas/parejaparaguas.jpg', width='100%', height='100%')
	
	section#div-6.slide
		section.info-portada
			div.container
				div.row-fluid.center
					<br>
					h3.texto-portada <strong><span class='tiny negrita'>u</span>Printoo</strong> te ayuda con las fotos que tomas.<br>Ólvidate de la flojera de ir a imprimirlas y dile <br> <span class="negrita">hola</span> a la comidad de recibirlas en tu casa.
					<br><br><br><br><br><br><br><br>
					div.span5.center.offset5.godown
						a.arrow-down(href='#div-7')
		section.img-portada
			//--img(src='/img/portadas/papahijo.jpg', width='100%', height='100%')

	section#div-7.slide
		section.info-features
			div.container
				div.row-fluid
					div.span11
						div.span4.feature.offset2
							div.span12.center
								div.tag
							h2.center.title-feature ¿Porqué es <br> gratis?
							div.info.span8.offset2
								//--p.center Es gratis porque detrás de las fotos <span class='sub-texto'>hay publicidad no invasiva y desprendible. </span> <br> Las marcas son las que pagan la impresión y envío de tus fotos.
						div.span4.feature.offset1
							div.span12.center
								div.gift
							h2.center.title-feature ¿En qué me <br> beneficia?
							div.info.span10.offset1
								//--ul
									li
										p Nunca vuelves a perder una buena foto.
									li
										p Toma una foto e inmediatamente mándala a imprimir.
									li
										p Puedes regalar fotos y postales a tus amigos y familiares.
					div.span12.center.godown
						a.arrow-down.offset5(href='#div-8')
		section.img-portada
			//--img(src='/img/portadas/velero.png', width='100%', height='100%')

	section#div-8.slide
		section.info-portada-ultima
			div.container
				div.row-fluid.center
					h3.texto-portada Regístrate para poder utilizar nuestra versón <span class="negrita"> Beta </span> y tener la <br> oportunidad de recibir <span class="negrita">fotos ilimitadas</span> de por vida.

				div.span10.newlester
					div.row-fluid.center
						div.registro-newlester.span10.offset1
							form.span12(method='POST', action='/registro/newlester', name='newlester_register_form')#newlester_register_form
								input#txtMail(type='email',name='txtMail', placeholder='Mi email es...' required='required')
								//--button.btn.btn-primary.registrar(type='submit', id='btnLoginNewlester') Registrar
								a.submit.registrar(href="#")
									img(src='/img/iconos/btn_enviar.png')


	include footer
